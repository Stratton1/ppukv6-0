# Property Passport UK - Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-anon-key-here

# Supabase Service Role (for server-side operations)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Development Settings
NODE_ENV=development
VITE_APP_ENV=development

# Optional: External API Keys (if needed)
# VITE_GOOGLE_MAPS_API_KEY=your-google-maps-key
# VITE_EPC_API_KEY=your-epc-api-key

# Database Connection (if using direct connection)
# DATABASE_URL=postgresql://user:password@host:port/database

# Storage Configuration
# VITE_STORAGE_BUCKET_PHOTOS=property-photos
# VITE_STORAGE_BUCKET_DOCUMENTS=property-documents
